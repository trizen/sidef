#
## http://rosettacode.org/wiki/Stack
#

class Stack(stack=[]) {
    method pop        { stack.pop };
    method push(item) { stack.push(item) };
    method empty      { stack.is_empty };
}
Â 
var stack = Stack();
stack.push(42);
say stack.pop;          # => 42
say stack.empty;        # => true
