#!/usr/bin/ruby

#
## https://rosettacode.org/wiki/Copy_a_string
#

var original = "hello";               # new String object
var reference = original;             # points at the original object
var copy1 = String.new(original);     # creates a new String object
var copy2 = original+'';              # ==//==

say reference;
say copy1;
say copy2;
