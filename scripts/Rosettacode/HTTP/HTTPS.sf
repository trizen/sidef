var lwp = require('LWP::UserAgent');    # LWP::Protocol::https is needed
var url = 'https://rosettacode.org';
Â 
var ua = lwp.new(
    agent    => 'Mozilla/5.0',
    ssl_opts => Hash.new(verify_hostname => 1),
);
Â 
var resp = ua.get(url);
resp.is_success
    || die "Failed to GET #{url.dump}: #{resp.status_line}\n";
print resp.decoded_content;
