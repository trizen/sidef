#!/usr/bin/ruby

# Tests for various String methods

do {

    var json = '{"a":42, "c":{"d":["foo", "bar", true, false, 42.9, 1234]}}'
    var hash = json.from_json
    var json2 = hash.to_json
    var hash2 = json2.from_json

    assert_eq(hash, hash2)
}

do {

    assert_eq(bwt_decode(bwt_encode("abracadabra")), "abracadabra")
    assert_eq(bwt_decode(bwt_encode("banana")), "banana")

}

say "** Tests passed!"
